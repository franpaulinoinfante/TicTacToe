@startuml TicTacToe Class Analysis
    !theme blueprint
    title TicTacToe Class' Analysis

    package TicTacToe{
        namespace Models{
            class TicTacToe{
                ~TicTacToe()
                ~Run()
                -ConfigurePlayer() 
                -Start()
                -Play()
                -IsResume() : boolean
            }
            TicTacToe *--> Board
            TicTacToe *--> Turn
            TicTacToe *--> "2" Player
            TicTacToe ..> PlayerConfiguration
        }
    }
@enduml

