@startuml Architecture Analysis
    !theme blueprint
    title Analysis Package and Entity

    actor User
    package ConsoleApp{
        package Models{
            entity TicTacToe
            entity Board
            entity Turn
            entity PlayerConfiguration
            entity Player
            entity UserPlayer
            entity MachinePlayer

            TicTacToe *--> Board
            TicTacToe *--> Turn
            TicTacToe *--> Player
            TicTacToe ..> PlayerConfiguration

            PlayerConfiguration *--> Player
            Player <|-- UserPlayer
            Player <|-- MachinePlayer
            Player --> Board

            Turn --> Player

        }
        Models ..> Types
        Models ..> Enums

        package Types{
            entity Coordinate
            entity ClosedInterval

            Coordinate ..> ClosedInterval
        }
        Types ..> Enums

        package Enums{
            entity Message
            entity Error
            entity Token
        }
    }
@enduml

